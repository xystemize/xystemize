import { AppGlobalState } from '../app-global-state/AppGlobalState';
import { Name } from '../constant';

const { accounts, apiv1, mediaItems } = Name;

class Url {
  private get baseUrl() {
    return `${AppGlobalState.firebaseApiBaseUrl}/${apiv1}`;
  }

  get accountsApiUrl() {
    return `${this.baseUrl}/${accounts}`;
  }
}

export const ApiUrl = new Url();
