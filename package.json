{
  "name": "@xystemize/source",
  "version": "1.0.0-beta.1",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install && echo \"npm run lintPrePush\" > .husky/pre-push",
    "lint": "npx nx run-many --all --skip-nx-cache --parallel --targets=lint,type-check --fix",
    "lintPrePush": "npx nx run-many --all --skip-nx-cache --parallel --targets=lint,type-check",
    "nxReleaseSetup": "npx nx generate nx-release:configure",
    "codeCheck": "npx prettier --check .",
    "codeFormat": "npx prettier --write . && npm run nxLintAndFix",
    "backendKillPorts": "npx nx run backend:killports",
    "backendTerminalTabEmulators": "npx nx run backend:terminalTabEmulators"
  },
  "private": true,
